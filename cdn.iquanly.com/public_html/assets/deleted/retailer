        //    //$scope.lblOtherTimeFilter = ($scope.custFilter.fromDate ? $filter('date')($scope.custFilter.fromDate, 'dd/MM/yyyy') : '') + " - " + ($scope.custFilter.toDate ? $filter('date')($scope.custFilter.toDate, 'dd/MM/yyyy') : '');
